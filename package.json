{
  "name": "hacka-news-web",
  "version": "1.0.0",
  "description": "hacka-news for the web",
  "main": "app.js",
  "scripts": {
    "postinstall": "yarn dl-vendors && npm run install-semantic && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node app.js",
    "install-semantic": "cd node_modules/fomantic-ui && gulp install",
    "build": "npm run build-semantic && npm run build-web",
    "build-semantic": "cd semantic && gulp build",
    "build-web": "webpack",
    "watch-web": "webpack -w",
    "dl-vendors": "./download-vendors.sh",
    "snyk-protect": "snyk-protect",
    "prepare": "yarn run snyk-protect"
  },
  "author": "James Cote",
  "license": "MIT",
  "dependencies": {
    "@snyk/protect": "^1.1091.0",
    "ejs": "^3.1.8",
    "ejs-lint": "^2.0.0",
    "express": "^4.18.2",
    "fomantic-ui": "^2.9.1",
    "hacka-news": "^1.2.0",
    "snyk": "^1.1091.0"
  },
  "devDependencies": {
    "browser-sync": "^2.27.11",
    "copy-webpack-plugin": "^6.0.3",
    "gulp": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-stream": "^5.1.1"
  },
  "engines": {
    "node": "^16.13.0"
  },
  "snyk": true
}
